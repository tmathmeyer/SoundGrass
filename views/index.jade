extends layout

block content
	div(class="station-name")
		h1=station
	audio(preload)
		source(src="/file.wav",type='audio/wav')
	div(class="playpause")
		img(id="play",style="display:none",src="/images/play.svg")
		img(id="pause",src="/images/pause.svg")
	h2(class="song-title") The final Count down
	progress(value="50",max="100")
	div(id="spacer")
	canvas(id="can")
